{
    "name": "createchatbot",
    "title": "Create Chatbot",
    "description": "Grow Your Business by Building Deeper Interactions With Your Customers.",
    "author": "Sergey Alekseev, Kiryl Ply",
    "homepage": "http://createchatbot.com",
    "vendors": [
        "./node_modules/bootstrap/js/src/index.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dmnsgn/gulp-frontend-boilerplate"
    },
    "issues": {
        "url": "https://github.com/dmnsgn/gulp-frontend-boilerplate/issues"
    },
    "keywords": [
        "gulp",
        "gulp4",
        "es6",
        "es2015",
        "boilerplate",
        "frontend",
        "tasks",
        "workflow",
        "preprocessor",
        "framework"
    ],
    "license": "MIT",
    "vendors": [],
    "napa": {},
    "dependencies": {
        "normalize.css": "git://github.com/necolas/normalize.css.git#3.0.3"
    },
    "devDependencies": {
        "aliasify": "^2.1.0",
        "autoprefixer": "^6.3.x",
        "babelify": "^8.x.x",
        "babel-core": "^6.7.x",
        "babel-preset-es2015": "^6.6.x",
        "brfs": "^1.4.3",
        "browser-sync": "^2.12.x",
        "browserify": "^13.3.x",
        "bootstrap": "4.0.0-beta.3",
        "jquery": "1.9.1",
        "popper.js": "^1.12.9",
        "chalk": "^1.1.x",
        "css-mqpacker": "^5.0.x",
        "csswring": "^5.0.x",
        "del": "^2.2.x",
        "eslint": "^3.6.1",
        "eslint-config-airbnb": "^12.0.x",
        "eslint-plugin-import": "^1.16.0",
        "eslint-plugin-import": "^2.8.x",
        "eslint-plugin-jsx-a11y": "^2.2.x",
        "eslint-plugin-react": "^6.3.x",
        "favicons": "^4.7.x",
        "fontmin": "^0.9.x",
        "gulp": "github:gulpjs/gulp#d8f5c90a0622d19ef1943a2a3d02dc50e3c853e7",
        "gulp-cache": "^0.4.x",
        "gulp-changed": "^1.3.x",
        "gulp-filter": "^4.0.x",
        "gulp-header": "^1.7.x",
        "gulp-imagemin": "^3.0.x",
        "gulp-mocha": "^3.0.x",
        "gulp-newer": "^1.1.x",
        "gulp-notify": "^2.2.x",
        "gulp-postcss": "^6.1.x",
        "gulp-preprocess": "^2.0.x",
        "gulp-rename": "^1.2.x",
        "gulp-sass": "^3.1.x",
        "gulp-sass-glob": "^1.0.8",
        "gulp-sitemap": "^4.0.x",
        "gulp-sourcemaps": "^1.6.x",
        "gulp-uglify": "^2.0.x",
        "gulp-util": "^3.0.x",
        "gulp-w3cjs": "^1.2.x",
        "gulp-watch": "^4.3.x",
        "gulp.spritesmith": "^6.2.x",
        "imagemin-pngquant": "^5.0.x",
        "minimist": "^1.2.x",
        "napa": "^2.x.x",
        "postcss": "^5.2.4",
        "pretty-hrtime": "^1.0.x",
        "psi": "^2.0.x",
        "stylelint": "^7.3.1",
        "stylelint-config-suitcss": "^8.0.0",
        "uglify-js": "^2.x.x",
        "vinyl-buffer": "^1.0.x",
        "vinyl-source-stream": "^2.0.0",
        "watchify": "^3.7.x"
    },
    "scripts": {
        "install": "napa",
        "init": "npm i inquirer@1.0.3 chalk cli-spinner del babel-cli babel-preset-es2015 && babel-node ./boilerplate/init/index.js",
        "dev": "NODE_ENV=development gulp --env dev",
        "prod": "NODE_ENV=production gulp --env prod"
    },
    "directories": {
        "src": "./src",
        "dist": "./dist",
        "test": "./test"
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    },
    "stylelint": {
        "extends": "stylelint-config-suitcss",
        "rules": {
            "rule-properties-order": null
        }
    },
    "eslintConfig": {
        "extends": "airbnb",
        "plugins": [
            "react"
        ],
        "globals": {
            "ga": true
        },
        "rules": {
            "comma-dangle": 0
        }
    },
    "config": {
        "verbose": false,
        "port": 3000,
        "browsers": [
            "last 2 version",
            "safari 5",
            "ie 9",
            "opera 12.1",
            "ios 6",
            "android 4"
        ],
        "prodURL": "http://createchatbot.com",
        "shareImageURL": "http://createchatbot.com/static/share.png",
        "twitterImage": "https://domain.tld/static/twitter.png",
        "twitterHandle": "@createchatbot",
        "themeColor": "#eeeeee",
        "analyticsUA": "UA-76869527-1",
        "developerURL": ""
    },
    "extensions": {
        "scripts": "js",
        "styles": "scss"
    },
    "browserify": {
        "transform": [
            "babelify",
            "aliasify",
            "brfs"
        ]
    },
    "aliasify": {
        "aliases": {
            "root": "./src/scripts",
            "views": "./src/scripts/views"
        }
    }
}